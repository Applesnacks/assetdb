	},
	"homepage": "https://github.com/alatif113/assetdb#readme",
	"dependencies": {
		"babel-cli": "^6.26.0",
		"terser": "^5.6.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.13.10",
		"@babel/core": "^7.13.10",
		"@babel/preset-env": "^7.13.10"
	}
}
